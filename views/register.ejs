<%- include('header'); -%>
    <form action="/api/register" method="post">
        <label for="username">Username:</label>
        <input type="text" id="username" name="username" placeholder="Enter your username">
        <br>
        <label for="password">Password:</label>
        <input type="password" id="password" name="password" placeholder="Minimum 8 characters">
        <br>
        <label for="society">Society:</label>
        <input type="text" id="society" name="society">
        <br>
        <label for="name">Phone Number:</label>
        <input type="tel" id="number" name="number">
        <br>
        <label for="address">Address:</label>
        <input type="text" id="address" name="address">
        <br>
        <label for="zipcode">Zipcode</label>
        <input type="number" id="zipcode" name="zipcode">
        <br>
        <label for="city">City:</label>
        <input type="text" id="city" name="city" placeholder="Enter a city">
        <br>
        <label for="fax">Fax:</label>
        <input type="number" id="fax" name="fax">
        <br>
        <label for="payment">Payment:</label>
        <input type="text" id="payment" name="payment">
        <br>
        <label for="max_outstanding">Max Outstanding:</label>
        <input type="text" id="max_outstanding" name="max_outstanding">
        <br>
        <button type="submit">Register</button>
    </form>
<%- include('footer'); -%>
